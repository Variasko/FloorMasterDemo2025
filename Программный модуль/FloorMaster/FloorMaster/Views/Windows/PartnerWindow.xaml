<Window x:Class="FloorMaster.Views.Windows.PartnerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FloorMaster.Views.Windows"
        mc:Ignorable="d"
        TextElement.FontFamily="Segoe UI"
        TextElement.FontSize="25"
        Title="Партнёры" Height="600" Width="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="75"/>
        </Grid.RowDefinitions>

        <ScrollViewer x:Name="PartnerScroll" VerticalScrollBarVisibility="Auto">
            
        </ScrollViewer>

        <Button x:Name="BackButton"
                Grid.Row="1"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Content="Назад"
                Margin="20 0"
                Padding="10 5"
                Click="BackButton_Click"/>
        <Button x:Name="AddButton"
                Grid.Row="1"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                Content="Добавить"
                Margin="20 0"
                Padding="10 5"
                Click="AddButton_Click"/>

    </Grid>
</Window>
